
public class testMap {

	public static void main(String[] args) {
		
		Map maMap = new Map(2,2);
		System.out.println("longueur de la map:" +maMap.getLongueur());
		System.out.println("largeur de la map:" +maMap.getLargeur());
		
		maMap.mettreSurLaMap(0, 0, 'o');
		maMap.mettreSurLaMap(1, 0, 'o');
		maMap.mettreSurLaMap(0, 1, 'o');
		maMap.mettreSurLaMap(1, 1, 'o');
		
		maMap.dessinerMap();
	
	}

}
