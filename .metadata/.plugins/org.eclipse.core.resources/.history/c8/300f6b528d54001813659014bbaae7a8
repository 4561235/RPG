
public class testMap {

	public static void main(String[] args) {
		
		Map maMap = new Map(5,5);
		System.out.println("longueur de la map:" +maMap.getLongueur());
		System.out.println("largeur de la map:" +maMap.getLargeur());
		
		maMap.mettreSurLaMap(0, 0, 'a');
		maMap.mettreSurLaMap(0, 1, 'a');
		maMap.mettreSurLaMap(0, 2, 'a');
		maMap.mettreSurLaMap(0, 3, 'a');
		maMap.mettreSurLaMap(0, 4, 'a');
		maMap.mettreSurLaMap(1, 0, 'b');
		maMap.mettreSurLaMap(1, 1, 'b');
		maMap.mettreSurLaMap(1, 2, 'b');
		maMap.mettreSurLaMap(1, 3, 'b');
		maMap.mettreSurLaMap(1, 4, 'b');

		

	
		
		maMap.dessinerMap();
	
	}

}
