
public class testMap {

	public static void main(String[] args) {
		
		Map maMap = new Map(3,3);
		System.out.println("longueur de la map:" +maMap.getLongueur());
		System.out.println("largeur de la map:" +maMap.getLargeur());
		
		maMap.mettreSurLaMap(0, 0, 'a');
		maMap.mettreSurLaMap(1, 0, 'a');
		maMap.mettreSurLaMap(2, 0, 'a');
		
		maMap.mettreSurLaMap(0, 1, 'a');
		maMap.mettreSurLaMap(1, 1, ' ');
		maMap.mettreSurLaMap(2, 1, 'a');
		
		maMap.mettreSurLaMap(0, 2, 'a');
		maMap.mettreSurLaMap(1, 2, 'a');
		maMap.mettreSurLaMap(2, 2, 'a');
		


	

		

	
		
		maMap.dessinerMap();
	
	}

}
